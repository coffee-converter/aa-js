    "type": "patreon",
    "url": "https://www.patreon.com/onekiloparsec"
  },
  "bugs": {
    "url": "https://github.com/onekiloparsec/AA.js/issues"
  },
  "homepage": "https://github.com/onekiloparsec/AA.js#readme",
  "dependencies": {
    "dayjs": "^1.9.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/types": "^7.11.5",
    "babel-eslint": "^10.1.0",
    "caniuse-lite": "^1.0.30001248",
    "eslint": "^7.32.0",
    "rimraf": "^3.0.2",
    "source-map-loader": "^1.1.1",
    "ts-loader": "^8.0.4",
    "typescript": "^4.0.3",
    "vite": "^2.7.13",
    "vitest": "^0.1.27"
